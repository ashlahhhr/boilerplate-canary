generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Perfume {
  id               Int      @id @default(autoincrement())
  name             String
  baseNote         String
  middleNote       String
  topNote          String
  smellDescription String
  gender           String
  occasion         String
  dayTime          String
  season           String
}
